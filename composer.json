{
    "name": "vinceg/php-uniterm-sdk",
    "type": "library",
    "description": "PHP Uniterm SDK",
    "keywords": [
        "php",
        "sdk",
        "uniterm"
    ],
    "homepage": "https://github.com/vinceg/php-uniterm-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Gabriel",
            "email": "vadimg88@gmail.com",
            "homepage": "https://github.com/vinceg",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "sabre/xml": "^3.0",
        "tightenco/collect": "^6|^7|^8|^9",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Uniterm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uniterm\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
